@article{HamainzaB2014ComparisonMobile,
  title = {Comparison of a Mobile Phone-Based Malaria Reporting System with Source Participant Register Data for Capturing Spatial and Temporal Trends in  Epidemiological Indicators of Malaria Transmission Collected by Community Health  Workers in Rural {{Zambia}}.},
  author = {Hamainza, Busiku and Killeen, Gerry F. and Kamuliwo, Mulakwa and Bennett, Adam and Yukich, Joshua O.},
  date = {2014-12-12},
  journaltitle = {Malaria journal},
