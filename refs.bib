@article{ChakiPP2009AchievingHigh,
  title = {Achieving High Coverage of Larval-Stage Mosquito Surveillance: Challenges for a Community-Based Mosquito Control Programme in Urban {{Dar}} Es {{Salaam}}, {{Tanzania}}.},
  author = {Chaki, Prosper P. and Govella, Nicodem J. and Shoo, Bryson and Hemed, Abdullah and Tanner, Marcel and Fillinger, Ulrike and Killeen, Gerry F.},
