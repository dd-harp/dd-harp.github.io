@article{KraemerMUG2015GlobalDistribution,
  title = {The Global Distribution of the Arbovirus Vectors {{{\emph{Aedes}}}}{\emph{ Aegypti}} and {{{\emph{Ae}}}}{\emph{. Albopictus}}},
  author = {Kraemer, Moritz U G and Sinka, Marianne E and Duda, Kirsten A and Mylne, Adrian Q N and Shearer, Freya M and Barker, Christopher M and Moore, Chester G and Carvalho, Roberta G and Coelho, Giovanini E and Van Bortel, Wim and Hendrickx, Guy and Schaffner, Francis and Elyazar, Iqbal R F and Teng, Hwa-Jen and Brady, Oliver J and Messina, Jane P and Pigott, David M and Scott, Thomas W and Smith, David L and Wint, G R William and Golding, Nick and Hay, Simon I},
