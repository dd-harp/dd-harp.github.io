@article{GriffingSM2015HistoricalPerspective,
  title = {A Historical Perspective on Malaria Control in {{Brazil}}},
  author = {Griffing, Sean Michael and Tauil, Pedro Luiz and Udhayakumar, Venkatachalam and Silva-Flannery, Luciana},
  date = {2015-09},
  journaltitle = {Mem. Inst. Oswaldo Cruz},
