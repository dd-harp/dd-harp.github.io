@article{YamanaTK2017HysteresisSimulations,
  title = {Hysteresis in Simulations of Malaria Transmission},
  author = {Yamana, Teresa K. and Qiu, Xin and Eltahir, Elfatih A. B.},
  date = {2017-10-01},
  journaltitle = {Advances in Water Resources},
  shortjournal = {Advances in Water Resources},
  volume = {108},
  pages = {416--422},
  issn = {0309-1708},
  doi = {10.1016/j.advwatres.2016.10.003},
  url = {https://www.sciencedirect.com/science/article/pii/S030917081630519X},
  urldate = {2023-06-26},
  abstract = {Malaria transmission is a complex system and in many parts of the world is closely related to climate conditions. However, studies on environmental determinants of malaria generally consider only concurrent climate conditions and ignore the historical or initial conditions of the system. Here, we demonstrate the concept of hysteresis in malaria transmission, defined as non-uniqueness of the relationship between malaria prevalence and concurrent climate conditions. We show the dependence of simulated malaria transmission on initial prevalence and the initial level of human immunity in the population. Using realistic time series of environmental variables, we quantify the effect of hysteresis in a modeled population. In a set of numerical experiments using HYDREMATS, a field-tested mechanistic model of malaria transmission, the simulated maximum malaria prevalence depends on both the initial prevalence and the initial level of human immunity in the population. We found the effects of initial conditions to be of comparable magnitude to the effects of interannual variability in environmental conditions in determining malaria prevalence. The memory associated with this hysteresis effect is longer in high transmission settings than in low transmission settings. Our results show that efforts to simulate and forecast malaria transmission must consider the exposure history of a location as well as the concurrent environmental drivers.},
  langid = {english},
  keywords = {Environmental drivers,HYDREMATS,Hysteresis,immunity,Initial conditions,Malaria,MBPT dynamics and control},
  file = {/Users/smitdave/Zotero/storage/LW3IE2U7/S030917081630519X.html}
}

