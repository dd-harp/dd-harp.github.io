@article{KilleenGF2017MeasuringManipulating,
  title = {Measuring, Manipulating and Exploiting Behaviours of Adult Mosquitoes to Optimise Malaria Vector Control Impact},
  author = {Killeen, Gerry F and Marshall, John M and Kiware, Samson S and South, Andy B and Tusting, Lucy S and Chaki, Prosper P and Govella, Nicodem J},
  date = {2017-04},
  journaltitle = {BMJ Glob Health},
  volume = {2},
