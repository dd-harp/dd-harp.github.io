@article{BurliC2018AnalysisInterventions,
  title = {Analysis of Interventions against the Liver Fluke, Opisthorchis Viverrini},
  author = {Bürli, Christine and Harbrecht, Helmut and Odermatt, Peter and Sayasone, Somphou and Chitnis, Nakul},
  date = {2018-09},
  journaltitle = {Math. Biosci.},
  volume = {303},
  eprint = {29958977},
  eprinttype = {pmid},
  pages = {115--125},
  issn = {0025-5564, 1879-3134},
  doi = {10.1016/j.mbs.2018.06.008},
  url = {http://dx.doi.org/10.1016/j.mbs.2018.06.008},
  abstract = {We adapt a population-based model of Opisthorchis viverrini transmission dynamics to determine the effectiveness of three different interventions. The model includes the definitive hosts, humans; the reservoir hosts, dogs and cats; and the intermediate hosts, snails and fish. We consider the interventions: education campaigns to reduce the consumption of raw or undercooked fish, improved sanitation and treatment through mass drug administration. We fit model parameters to a data set from two islands in southern Lao PDR. We calculate the control reproduction number, simulate different scenarios and optimise the interventions with optimal control. We look at the potential of the interventions to eliminate transmission within 20 years. The model shows that education and improved sanitation need a very high coverage to fulfil the goal of elimination, whereas annual drug distribution at medium coverage is sufficient. The best solution is a combination of drug distribution at a medium level of coverage and as high as possible coverage of education and improved sanitation.},
  langid = {english},
  keywords = {dynamic model,Intervention,Mathematical modelling,Opisthorchis viverrini,Optimal control,Simulation},
  file = {/Users/smitdave/Zotero/storage/HDBN2YDX/Bürli et al. 2018 - Analysis of interventions against the liver fluke, opisthorchis viverrini.pdf}
}

@article{BurliC2018MathematicalAnalysis,
