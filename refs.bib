@article{MacdonaldG1968DynamicsMalaria,
  title = {The Dynamics of Malaria},
  author = {Macdonald, George and Cuellar, C B and Foll, C V},
  date = {1968-02},
  journaltitle = {Bull. World Health Organ.},
  volume = {38},
  number = {5},
  eprint = {5303328},
  eprinttype = {pmid},
  pages = {743--755},
  issn = {0042-9686},
  url = {https://www.ncbi.nlm.nih.gov/pubmed/5303328},
  abstract = {Previous studies on dynamic systems of transmission of malaria, and of eradication of infection following the interruption of transmission, have now been adapted for advanced techniques using the facilities offered by computers.The computer programmes have been designed for a deterministic model suitable for a large community and also for a stochastic model relevant to small populations in which infections reach very low finite numbers. In this model, new infections and recoveries are assessed by the daily inoculation rate and are subject to laws of chance. Such a representation is closer than previous models to natural happenings in the process of malaria eradication. Further refinements of the new approach include the seasonal transmission and simulation of mass chemotherapy aimed at a cure of P. falciparum infections.These programmes present models on which the actual or expected results of changes due to various factors can be studied by the analysis of specific malaria situations recorded in the field. The value of control methods can also be tested by the study of such hypothetical epidemiological models and by trying out various procedures.Two specific malaria situations (in a pilot project in Northern Nigeria and in an outbreak in Syria) were studied by this method and provided some interesting results of operational value. The attack measures in the pilot project in Northern Nigeria were carried out according to the theoretical model derived from the basic data obtained in the field.},
  langid = {english},
  keywords = {dynamic model,Dynamical Perspective,George Macdonald,malaria,malaria model,Malaria Models (1897-1969),Plasmodium falciparum},
  file = {/Users/smitdave/Zotero/storage/654CPZ26/Macdonald et al. - 1968 - The dynamics of malaria.pdf;/Users/smitdave/Zotero/storage/AQMQSKFT/Macdonald et al. 1968 - The dynamics of malaria.pdf}
}

