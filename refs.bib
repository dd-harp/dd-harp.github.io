@article{YakobL2009ModelingEffectsa,
  title = {Modeling the {{Effects}} of {{Integrating Larval Habitat Source Reduction}} and {{Insecticide Treated Nets}} for {{Malaria Control}}},
  author = {Yakob, Laith and Yan, Guiyun},
  editor = {Snounou, Georges},
  date = {2009-09-09},
  journaltitle = {PLoS ONE},
