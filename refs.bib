@article{HarrisAD2004CocarriageRates,
  title = {Co-Carriage Rates of Vancomycin-Resistant {{Enterococcus}} and Extended-Spectrum Beta-Lactamase-Producing Bacteria among a Cohort of Intensive Care Unit Patients: Implications for an Active Surveillance Program},
  author = {Harris, Anthony D and Nemoy, Lucia and Johnson, Judith A and Martin-Carnahan, Amy and Smith, David L and Standiford, Hal and Perencevich, Eli N},
  date = {2004-02},
  journaltitle = {Infect. Control Hosp. Epidemiol.},
