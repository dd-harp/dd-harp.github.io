@article{ChitnisN2006BifurcationAnalysis,
  title = {Bifurcation Analysis of a Mathematical Model for Malaria Transmission},
  author = {Chitnis, Nakul and Cushing, J M and Hyman, J M},
  date = {2006-01},
  journaltitle = {SIAM J. Appl. Math.},
