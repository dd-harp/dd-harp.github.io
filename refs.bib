@article{SokhnaCS2001RapidReappearance,
  title = {Rapid Reappearance of {{{\emph{Plasmodium}}}}{\emph{ Falciparum}} after Drug Treatment among {{Senegalese}} Adults Exposed to Moderate Seasonal Transmission},
