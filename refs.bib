}

@article{PatelJC2014GeneticEvidence,
  title = {Genetic {{Evidence}} of {{Importation}} of {{Drug-Resistant Plasmodium}} Falciparum to {{Guatemala}} from the {{Democratic Republic}} of the {{Congo}}},
