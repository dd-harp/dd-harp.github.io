@article{CoffengLE2017PowerMalaria,
  title = {The {{Power}} of {{Malaria Vaccine Trials Using Controlled Human Malaria Infection}}},
  author = {Coffeng, Luc E. and Hermsen, Cornelus C. and Sauerwein, Robert W. and family=Vlas, given=Sake J., prefix=de, useprefix=false},
  date = {2017-01-12},
  journaltitle = {PLOS Computational Biology},
