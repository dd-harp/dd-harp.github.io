@article{BattleKE2015GlobalDatabase,
  title = {Global Database of Matched {{{\emph{Plasmodium}}}}{\emph{ Falciparum}} and {{{\emph{P}}}}{\emph{. Vivax}} Incidence and Prevalence Records from 1985-2013},
  author = {Battle, Katherine E and Guerra, Carlos A and Golding, Nick and Duda, Kirsten A and Cameron, Ewan and Howes, Rosalind E and Elyazar, Iqbal R F and Baird, J Kevin and Reiner, Jr, Robert C and Gething, Peter W and Smith, David L and Hay, Simon I},
