@article{WatsonOJ2017ModellingDrivers,
  title = {Modelling the Drivers of the Spread of {{Plasmodium}} Falciparum Hrp2 Gene Deletions in Sub-{{Saharan Africa}}},
  author = {Watson, Oliver J and Slater, Hannah C and Verity, Robert and Parr, Jonathan B and Mwandagalirwa, Melchior K and Tshefu, Antoinette and Meshnick, Steven R and Ghani, Azra C},
  date = {2017-08},
  journaltitle = {Elife},
  volume = {6},
  eprint = {28837020},
  eprinttype = {pmid},
  pages = {19},
  issn = {2050-084X},
  doi = {10.7554/eLife.25008},
  url = {http://dx.doi.org/10.7554/eLife.25008},
  abstract = {Rapid diagnostic tests (RDTs) have transformed malaria diagnosis. The most prevalent P. falciparum RDTs detect histidine-rich protein 2 (PfHRP2). However, pfhrp2 gene deletions yielding false-negative RDTs, first reported in South America in 2010, have been confirmed in Africa and Asia. We developed a mathematical model to explore the potential for RDT-led diagnosis to drive selection of pfhrp2-deleted parasites. Low malaria prevalence and high frequencies of people seeking treatment resulted in the greatest selection pressure. Calibrating our model against confirmed pfhrp2-deletions in the Democratic Republic of Congo, we estimate a starting frequency of 6\% pfhrp2-deletion prior to RDT introduction. Furthermore, the patterns observed necessitate a degree of selection driven by the introduction of PfHRP2-based RDT-guided treatment. Combining this with parasite prevalence and treatment coverage estimates, we map the model-predicted spread of pfhrp2-deletion, and identify the geographic regions in which surveillance for pfhrp2-deletion should be prioritised.},
  langid = {english},
  keywords = {dynamic model,epidemiology,global health,mapping,mathematical modelling,pfhrp2-deletion,Plasmodium falciparum,rapid diagnostic tests},
  file = {/Users/smitdave/Zotero/storage/WISVXW77/Watson et al. 2017 - Modelling the drivers of the spread of Plasmodium falciparum hrp2 gene deletions in sub-Saharan Africa.pdf}
}

@article{WatsonOJ2019FalsenegativeMalaria,
  title = {False-Negative Malaria Rapid Diagnostic Test Results and Their Impact on Community-Based Malaria Surveys in Sub-{{Saharan Africa}}},
  author = {Watson, Oliver J and Sumner, Kelsey Marie and Janko, Mark and Goel, Varun and Winskill, Peter and Slater, Hannah C and Ghani, Azra and Meshnick, Steven R and Parr, Jonathan B},
  date = {2019-07},
  journaltitle = {BMJ Glob Health},
  volume = {4},
  number = {4},
  eprint = {31406591},
  eprinttype = {pmid},
  pages = {e001582},
  issn = {2059-7908},
  doi = {10.1136/bmjgh-2019-001582},
  url = {http://dx.doi.org/10.1136/bmjgh-2019-001582},
  abstract = {Surveillance and diagnosis of Plasmodium falciparum malaria relies predominantly on rapid diagnostic tests (RDT). However, false-negative (FN) RDT results are known to occur for a variety of reasons, including operator error, poor storage conditions, pfhrp2/3 gene deletions, poor performance of specific RDT brands and lots, and low-parasite density infections. We used RDT and microscopy results from 85 000 children enrolled in Demographic Health Surveys and Malaria Indicator Surveys from 2009 to 2015 across 19 countries to explore the distribution of and risk factors for FN-RDTs in sub-Saharan Africa, where malaria's impact is greatest. We sought to (1) identify spatial and demographic patterns of FN-RDT results, defined as a negative RDT but positive gold standard microscopy test, and (2) estimate the percentage of infections missed within community-based malaria surveys due to FN-RDT results. Across all studies, 19.9\% (95\% CI 19.0\% to 20.9\%) of microscopy-positive subjects were negative by RDT. The distribution of FN-RDT results was spatially heterogeneous. The variance in FN-RDT results was best explained by the prevalence of malaria, with an increase in FN-RDT results observed at lower transmission intensities, among younger subjects, and in urban areas. The observed proportion of FN-RDT results was not predicted by differences in RDT brand or lot performance alone. These findings characterise how the probability of detection by RDTs varies in different transmission settings and emphasise the need for careful interpretation of prevalence estimates based on surveys employing RDTs alone. Further studies are needed to characterise the cost-effectiveness of improved malaria diagnostics (eg, PCR or highly sensitive RDTs) in community-based surveys, especially in regions of low transmission intensity or high urbanicity.},
  langid = {english},
  keywords = {dynamic model,Imperial Model,malaria,malaria diagnosis,mapping,mathematical modelling,MBPT dynamics and control,pfhrp2,pfhrp2 deletion,Plasmodium falciparum,rapid diagnostic tests,RDTs},
  file = {/Users/smitdave/Zotero/storage/47VXGK8T/Watson et al. 2019 - False-negative malaria rapid diagnostic test result ... their impact on community-based malaria surveys in sub-Saharan Africa.pdf}
}

@article{WatsonOJ2019ImpactSeasonal,
  title = {Impact of Seasonal Variations in {{Plasmodium}} Falciparum Malaria Transmission on the Surveillance of Pfhrp2 Gene Deletions},
  author = {Watson, Oliver John and Verity, Robert and Ghani, Azra C and Garske, Tini and Cunningham, Jane and Tshefu, Antoinette and Mwandagalirwa, Melchior K and Meshnick, Steven R and Parr, Jonathan B and Slater, Hannah C},
  date = {2019-05},
  journaltitle = {Elife},
  volume = {8},
  eprint = {31045490},
