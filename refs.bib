  keywords = {Blood,Malaria,Malarial parasites,MBPT dynamics and control,mosquitoes,Oviposition,Parasitic diseases,Salivary glands,vector-borne diseases},
  file = {/Users/smitdave/Zotero/storage/DWKI47RT/Paaijmans et al. (2013) Temperature-Dependent Pre-Bloodmeal Period and Tem.pdf}
}

@article{PaaijmansKP2013TemperatureVariation,
