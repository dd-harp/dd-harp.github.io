}

@article{GuW2006SourceReduction,
  title = {Source Reduction of Mosquito Larval Habitats Has Unexpected Consequences on Malaria Transmission},
  author = {Gu, Weidong and Regens, James L. and Beier, John C. and Novak, Robert J.},
  date = {2006-11-14},
  journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
  shortjournal = {Proc Natl Acad Sci U S A},
  volume = {103},
  number = {46},
  eprint = {17085587},
  eprinttype = {pmid},
  pages = {17560--17563},
  issn = {0027-8424},
  doi = {10.1073/pnas.0608452103},
  abstract = {Reduction of aquatic habitats through environmental management mitigates malaria transmission not only by reducing emergence of host-seeking mosquitoes, but also by increasing the amount of time required for vectors to locate oviposition sites. However, the consequence of source reduction on mosquito oviposition has largely been neglected in evaluations of environment-management programs. Here, by theoretically examining the relationship between the time spent for oviposition and the availability of aquatic habitats, we show that prolonged oviposition cycles induced by source reduction account for a great deal of reductions in the basic reproductive rate of malaria, especially when aquatic habitats are scarce and the mosquito's flight ability is limited. Neglecting this mechanism may lead to substantial underestimation of the impact of source reduction of aquatic habitats on malaria transmission. Our findings suggest that the prolonged duration of the gonotrophic cycle might be one of the important mechanisms underlying the effectiveness of environment-management interventions for malaria control.},
  langid = {english},
  pmcid = {PMC1634411},
  keywords = {Animals,Culicidae,{Disease Transmission, Infectious},Dispersal in Context,Ecosystem,female,Larva,LSM,malaria,Malaria,MBPT dynamics and control,{Models, Biological},Oviposition,Plasmodium,Reproduction,Systematic Review (1970-2010),Vector Control,Vector Infection Dynamics},
  file = {/Users/smitdave/Zotero/storage/6H4V54J3/Gu et al. - 2006 - Source reduction of mosquito larval habitats has u.pdf;/Users/smitdave/Zotero/storage/JYPVV5NI/Gu et al. (2006) Source reduction of mosquito larval habitats has u.pdf;/Users/smitdave/Zotero/storage/T4GDQRZX/Gu et al. 2006 - Source reduction of mosquito larval habitats has unexpected consequences on malaria transmission.pdf}
}

@article{GuW2008HabitatbasedLarval,
  title = {Habitat-Based Larval Interventions: A New Perspective for Malaria Control},
  author = {Gu, Weidong and Utzinger, Jürg and Novak, Robert J},
  date = {2008-01},
  journaltitle = {Am. J. Trop. Med. Hyg.},
  volume = {78},
  number = {1},
  eprint = {18187774},
  eprinttype = {pmid},
  pages = {2--6},
  publisher = {{researchgate.net}},
  issn = {0002-9637},
  url = {https://www.ncbi.nlm.nih.gov/pubmed/18187774},
  abstract = {Interest in environmental management of mosquito larval habitats has been rekindled due to deterioration of malaria in tropical Africa. Environmental management programs were typically implemented as “all-out” campaigns by treating all potential breeding habitats. In contrast, targeted environmental management is based on a sound understanding of the heterogeneity in mosquito productivity. However, deficiencies in field methodology for measuring productivity hamper our progress in understanding of mosquito productivity. To address these issues, we develop a framework of habitat-based interventions by adoption of a landscape approach to elucidate mechanisms underlying mosquito productivity. The importance of vigorously quantitative estimation of the productivity is highlighted. Spatial models are proposed to examine the interrelationship between mosquito productivity and oviposition of gravid mosquitoes. In our view, environmental management approaches must take into account variability in productivity, in efforts to improve feasibility, cost-effectiveness, and sustainability of such approaches, particularly when implemented along with other malaria control measures.},
  langid = {english},
  keywords = {Dispersal in Context,malaria},
  file = {/Users/smitdave/Zotero/storage/3J2MSCU6/Gu et al. 2008 - Habitat-based larval interventions - a new perspective for malaria control.pdf}
}

