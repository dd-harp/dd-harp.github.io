@article{SattlerMA2005HabitatCharacterization,
  title = {Habitat Characterization and Spatial Distribution of {{Anopheles}} Sp. Mosquito Larvae in {{Dar}} Es {{Salaam}} ({{Tanzania}}) during an Extended Dry Period.},
  author = {Sattler, Michael A. and Mtasiwa, Deo and Kiama, Michael and Premji, Zul and Tanner, Marcel and Killeen, Gerry F. and Lengeler, Christian},
  date = {2005-01-14},
  journaltitle = {Malaria journal},
