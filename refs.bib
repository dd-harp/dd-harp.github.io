@article{KraemerMUG2015BigCity,
  title = {Big City, Small World: Density, Contact Rates, and Transmission of Dengue across {{Pakistan}}},
  shorttitle = {Big City, Small World},
  author = {Kraemer, M. U. G. and Perkins, T. A. and Cummings, D. a. T. and Zakar, R. and Hay, S. I. and Smith, D. L. and Reiner, R. C.},
  date = {2015-10-06},
  journaltitle = {Journal of The Royal Society Interface},
  shortjournal = {Journal of The Royal Society Interface},
  volume = {12},
