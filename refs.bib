@article{Garrett-JonesC1980FeedingHabits,
  title = {Feeding Habits of Anophelines ({{Diptera}}: {{Culicidae}}) in 1971–78, with Reference to the Human Blood Index: A Review},
  author = {Garrett-Jones, C and Boreham, P F L and Pant, C P},
