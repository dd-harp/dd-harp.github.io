@article{TrapeJF1998ImpactChloroquine,
  title = {Impact of Chloroquine Resistance on Malaria Mortality},
  author = {Trape, Jean-François and Pison, Gilles and Preziosi, Marie-Pierre and Enel, Catherine and family=Loû, given=Annabel Desgrées, prefix=du, useprefix=true and Delaunay, Valérie and Samb, Badara and Lagarde, Emmanuel and Molez, Jean-François and Simondon, François},
