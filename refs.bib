@article{LeMenachA2007_ITN,
  title = {An Elaborated Feeding Cycle Model for Reductions in Vectorial Capacity of Night-Biting Mosquitoes by Insecticide-Treated Nets},
  author = {Le Menach, Arnaud and Takala, Shannon L and McKenzie, F Ellis and Perisse, Andre and Harris, Anthony and Flahault, Antoine and Smith, David L},
