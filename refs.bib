@article{NambanyaS2010HighHeterogeneity,
  title = {High Heterogeneity in {{Plasmodium}} Falciparum Risk Illustrates the Need for Detailed Mapping to Guide Resource Allocation: A New Malaria Risk Map of the {{Lao People}}'s {{Democratic Republic}}},
  author = {Nambanya, Simone and Gopinath, Deyer and Hongvanthong, Bouasy and Bell, David and Phompida, Samlane and Phetsouvanh, Rattanaxay},
  date = {2010},
  journaltitle = {Malar. J.},
  volume = {9},
