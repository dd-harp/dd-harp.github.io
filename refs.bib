@article{MolineauxL2001PlasmodiumFalciparum,
  title = {\emph{Plasmodium Falciparum} Parasitaemia Described by a New Mathematical Model},
  author = {Molineaux, Louis and Diebner, H H and Eichner, Martin and Collins, W E and Jeffery, G M and Dietz, Klaus},
