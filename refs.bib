@article{OkandaFM2002BehaviouralDeterminants,
  title = {Behavioural Determinants of Gene Flow in Malaria Vector Populations: {{Anopheles}} Gambiae Males Select Large Females as Mates.},
  author = {Okanda, F. M. and Dao, A. and Njiru, B. N. and Arija, J. and Akelo, H. A. and Touré, Y. and Odulaja, A. and Beier, J. C. and Githure, J. I. and Yan, G. and Gouagna, L. C. and Knols, B. G. J. and Killeen, G. F.},
  date = {2002-08-14},
  journaltitle = {Malaria journal},
  shortjournal = {Malar J},
  volume = {1},
  eprint = {12296972},
  eprinttype = {pmid},
  pages = {10},
  location = {{England}},
  issn = {1475-2875},
  doi = {10.1186/1475-2875-1-10},
  abstract = {BACKGROUND: Plasmodium-refractory mosquitoes are being rapidly developed for malaria control but will only succeed if they can successfully compete for mates  when released into the wild. Pre-copulatory behavioural traits maintain genetic  population structure in wild mosquito populations and mating barriers have foiled  previous attempts to control malaria vectors through sterile male release.  METHODS: Varying numbers of virgin male and female Anopheles gambiae Giles, from  two strains of different innate sizes, were allowed to mate under standardized  conditions in laboratory cages, following which, the insemination status,  oviposition success and egg batch size of each female was assessed. The influence  of male and female numbers, strain combination and female size were determined  using logistic regression, correlation analysis and a simple mechanistic model of  male competition for females. RESULTS: Male An. gambiae select females on the  basis of size because of much greater fecundity among large females. Even under  conditions where large numbers of males must compete for a smaller number of  females, the largest females are more likely to become inseminated, to  successfully oviposit and to produce large egg batches. CONCLUSIONS: Sexual  selection, on the basis of size, could either promote or limit the spread of  malaria-refractory genes into wild populations and needs to be considered in the  continued development and eventual release of transgenic vectors. Fundamental  studies of behavioural ecology in malaria vectors such as An. gambiae can have  important implications for malaria control and should be prioritised for more  extensive investigation in the future.},
  langid = {english},
  pmcid = {PMC140138},
  keywords = {{*Sexual Behavior, Animal},Animals,Anopheles,Anopheles/genetics/*physiology,Body Constitution,female,gambiae,Insect Vectors/genetics/*physiology,Malaria,Male,Plasmodium}
}

@article{OkechBA2003InfluenceSugar,
  title = {Influence of {{Sugar Availability}} and {{Indoor Microclimate}} on {{Survival}} of {{Anopheles}} Gambiae ({{Diptera}}: {{Culicidae}}) {{Under Semifield Conditions}} in {{Western Kenya}}},
  shorttitle = {Influence of {{Sugar Availability}} and {{Indoor Microclimate}} on {{Survival}} of {{Anopheles}} Gambiae ({{Diptera}}},
  author = {Okech, Bernard A. and Gouagna, Louis C. and Killeen, Gerard F. and Knols, Bart G. J. and Kabiru, Ephantus W. and Beier, John C. and Yan, Guiyan and Githure, John I.},
  date = {2003-09-01},
  journaltitle = {Journal of Medical Entomology},
  shortjournal = {Journal of Medical Entomology},
  volume = {40},
  number = {5},
  pages = {657--663},
  issn = {0022-2585},
  doi = {10.1603/0022-2585-40.5.657},
  url = {https://doi.org/10.1603/0022-2585-40.5.657},
  urldate = {2023-06-26},
  abstract = {The influence of indoor microclimate on survival of female Anopheles gambiae sensu stricto Giles (Diptera: culicidae) mosquitoes fed on different nutrition sources was evaluated in a semifield experimental hut exposed to ambient climate in western Kenya. Cages of mosquitoes (n ≈ 50 per cage) were placed in nine positions within the hut combining three different sides and three different heights. At each height and side, mosquitoes were offered either human blood (once every 2 d), glucose (6\% wt:vol) or a combination of the two diets over three experiments so that each cage position received one diet source. The effect of diet on survival was significant with mean survival times of 14 d for mosquitoes fed blood alone, 29 d for sugar alone and 33 d for blood plus sugar. Sugar availability decreased the odds of mortality ≈85\% compared with the blood group. Micro heterogeneities of temperature but not relative humidity also influenced survival although to a much lesser extent. The side but not height within the hut at which mosquitoes were placed, influenced survival but could not be explained by either temperature or relative humidity differences. The potential influence of seemingly minor heterogeneities of indoor microclimate upon vector longevity and vectorial capacity may merit further investigation. Also, the availability of sugar was shown to be a potentially crucial determinant of vectorial capacity. Compared with blood alone, the availability of sugar served to increase survival potential of vectors beyond ages at which they are old enough to transmit malaria.},
  keywords = {*Housing,*Microclimate,*Sucrose,Animals,Anopheles,Anopheles physiology,blood feeding behavior,female,gambiae,Humans,Kenya,MBPT dynamics and control,Population Density},
  file = {/Users/smitdave/Zotero/storage/BVEKI4W6/Okech et al. (2003) Influence of Sugar Availability and Indoor Microcl.pdf;/Users/smitdave/Zotero/storage/FL3YSUAN/863449.html}
}

@article{OkellLC2017MappingSulphadoxinepyrimethamineresistant,
  title = {Mapping Sulphadoxine-Pyrimethamine-Resistant {{{\emph{Plasmodium}}}}{\emph{ Falciparum}} Malaria in Infected Humans and in Parasite Populations in {{Africa}}},
  author = {Okell, Lucy C and Griffin, Jamie T and Roper, Cally},
  date = {2017-08},
  journaltitle = {Sci. Rep.},
  volume = {7},
