  keywords = {Anopheles,gambiae},
  file = {/Users/smitdave/Zotero/storage/KW3UVY5Z/Zappia and Roitberg 2012 - Energy-state dependent responses of Anopheles gambiae (Diptera - Culicidae) to simulated bednet-protected hosts.pdf}
