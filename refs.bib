@article{YamanaTK2013IncorporatingEffects,
  title = {Incorporating the Effects of Humidity in a Mechanistic Model of {{{\emph{Anopheles}}}}{\emph{ Gambiae}} Mosquito Population Dynamics in the {{Sahel}} Region of {{Africa}}},
  author = {Yamana, Teresa K and Eltahir, Elfatih A B},
