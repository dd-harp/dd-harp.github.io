}

@article{EikenberrySE2018MathematicalModeling,
  title = {Mathematical Modeling of Climate Change and Malaria Transmission Dynamics: A Historical Review},
  shorttitle = {Mathematical Modeling of Climate Change and Malaria Transmission Dynamics},
  author = {Eikenberry, Steffen E. and Gumel, Abba B.},
  date = {2018-10-01},
  journaltitle = {Journal of Mathematical Biology},
  shortjournal = {J. Math. Biol.},
  volume = {77},
  number = {4},
  pages = {857--933},
  issn = {1432-1416},
  doi = {10.1007/s00285-018-1229-7},
  url = {https://doi.org/10.1007/s00285-018-1229-7},
  urldate = {2023-06-26},
  abstract = {Malaria, one of the greatest historical killers of mankind, continues to claim around half a million lives annually, with almost all deaths occurring in children under the age of five living in tropical Africa. The range of this disease is limited by climate to the warmer regions of the globe, and so anthropogenic global warming (and climate change more broadly) now threatens to alter the geographic area for potential malaria transmission, as both the Plasmodium malaria parasite and Anopheles mosquito vector have highly temperature-dependent lifecycles, while the aquatic immature Anopheles habitats are also strongly dependent upon rainfall and local hydrodynamics. A wide variety of process-based (or mechanistic) mathematical models have thus been proposed for the complex, highly nonlinear weather-driven Anopheles lifecycle and malaria transmission dynamics, but have reached somewhat disparate conclusions as to optimum temperatures for transmission, and the possible effect of increasing temperatures upon (potential) malaria distribution, with some projecting a large increase in the area at risk for malaria, but others predicting primarily a shift in the disease’s geographic range. More generally, both global and local environmental changes drove the initial emergence of P. falciparum as a major human pathogen in tropical Africa some 10,000~years ago, and the disease has a long and deep history through the present. It is the goal of this paper to review major aspects of malaria biology, methods for formalizing these into mathematical forms, uncertainties and controversies in proper modeling methodology, and to provide a timeline of some major modeling efforts from the classical works of Sir Ronald Ross and George Macdonald through recent climate-focused modeling studies. Finally, we attempt to place such mathematical work within a broader historical context for the “million-murdering Death” of malaria.},
  langid = {english},
  keywords = {climate change,malaria,Malaria,MBPT,MBPT dynamics and control,Plasmodium falciparum,review,Ross–Macdonald,temperature,Thermal-response},
  file = {/Users/smitdave/Zotero/storage/PFMDRKAL/Eikenberry and Gumel (2018) Mathematical modeling of climate change and malari.pdf}
}

@article{EisenbergJNS2012InroadsSpread,
  title = {In-Roads to the Spread of Antibiotic Resistance: Regional Patterns of Microbial Transmission in Northern Coastal {{Ecuador}}},
  shorttitle = {In-Roads to the Spread of Antibiotic Resistance},
  author = {Eisenberg, Joseph N. S. and Goldstick, Jason and Cevallos, William and Trueba, Gabriel and Levy, Karen and Scott, James and Percha, Bethany and Segovia, Rosana and Ponce, Karina and Hubbard, Alan and Marrs, Carl and Foxman, Betsy and Smith, David L. and Trostle, James},
  date = {2012-05-07},
  journaltitle = {Journal of The Royal Society Interface},
  shortjournal = {J. R. Soc. Interface.},
  volume = {9},
  number = {70},
  pages = {1029--1039},
  issn = {1742-5689, 1742-5662},
  doi = {10.1098/rsif.2011.0499},
  url = {https://royalsocietypublishing.org/doi/10.1098/rsif.2011.0499},
  urldate = {2022-12-10},
  abstract = {The evolution of antibiotic resistance (AR) increases treatment cost and probability of failure, threatening human health worldwide. The relative importance of individual antibiotic use, environmental transmission and rates of introduction of resistant bacteria in explaining community AR patterns is poorly understood. Evaluating their relative importance requires studying a region where they vary. The construction of a new road in a previously roadless area of northern coastal Ecuador provides a valuable natural experiment to study how changes in the social and natural environment affect the epidemiology of resistant               Escherichia coli               . We conducted seven bi-annual 15 day surveys of AR between 2003 and 2008 in 21 villages. Resistance to both ampicillin and sulphamethoxazole was the most frequently observed profile, based on antibiogram tests of seven antibiotics from 2210 samples. The prevalence of enteric bacteria with this resistance pair in the less remote communities was 80 per cent higher than in more remote communities (OR = 1.8 [1.3, 2.3]). This pattern could not be explained with data on individual antibiotic use. We used a transmission model to help explain this observed discrepancy. The model analysis suggests that both transmission and the rate of introduction of resistant bacteria into communities may contribute to the observed regional scale AR patterns, and that village-level antibiotic use rate determines which of these two factors predominate. While usually conceived as a main effect on individual risk, antibiotic use rate is revealed in this analysis as an effect modifier with regard to community-level risk of resistance.},
  langid = {english},
  keywords = {antibiotic resistance,article,Bibliographies/Books,DLS,mine,spatial dynamic model},
  file = {/Users/smitdave/Zotero/storage/AZPU8IWK/Eisenberg et al. - 2011 - In-roads to the spread of antibiotic resistance r.pdf;/Users/smitdave/Zotero/storage/Y66TGCXI/Eisenberg et al. 2012 - In-roads to the spread of antibiotic resistance - regional patterns of microbial transmission in northern coastal Ecuador.pdf;/Users/smitdave/Zotero/storage/Y8LVL2V2/Eisenberg et al. - 2012 - In-roads to the spread of antibiotic resistance r.pdf}
}

@article{EisenDP2002AlterationsPlasmodium,
  title = {Alterations in {{{\emph{Plasmodium}}}}{\emph{ Falciparum}} Genotypes during Sequential Infections Suggest the Presence of Strain Specific Immunity},
  author = {Eisen, Damon P and Saul, Allan and Fryauff, David J and Reeder, John C and Coppel, Ross L},
