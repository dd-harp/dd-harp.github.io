@article{KituaAY1996PlasmodiumFalciparum,
  title = {\emph{Plasmodium Falciparum} Malaria in the First Year of Life in an Area of Intense and Perennial Transmission},
  author = {Kitua, A Y and Smith, T and Alonso, P L and Masanja, H and Urassa, H and Menendez, C and Kimario, J and Tanner, M},
