@article{ReddyEA2010CommunityacquiredBloodstream,
  title = {Community-Acquired Bloodstream Infections in {{Africa}}: A Systematic Review and Meta-Analysis},
  author = {Reddy, Elizabeth A and Reddy, Elizabeth A and Shaw, Andrea V and Shaw, Andrea V and Crump, John A and Crump, John A},
