  keywords = {Africa,Africa South Of The Sahara,Child,Climate,Developing Countries,Disease Reservoirs,Diseases,Environment,Evaluation,French Speaking Africa,Geographic Factors,Geography,Humans,Malaria,Mali,MARA,{Models, Statistical},Observer Variation,Parasitic Diseases,Population,Population Density,Prevalence,Reproducibility of Results,Research Methodology,Research Report,Retrospective Studies,Risk Assessment,Seasons,Statistical Studies,Studies,Western Africa},
