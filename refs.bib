}

@article{MartensWJM1995ClimateChange,
  title = {Climate Change and Malaria: Exploring the Risks},
  author = {Martens, Willem J M},
  date = {1995-10},
  journaltitle = {Med. War},
  volume = {11},
  number = {4},
  eprint = {8559118},
