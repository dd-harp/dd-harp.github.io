@article{NamugangaJF2021ImpactStopping,
  title = {The Impact of Stopping and Starting Indoor Residual Spraying on Malaria Burden in {{Uganda}}},
  author = {Namuganga, Jane F and Epstein, Adrienne and Nankabirwa, Joaniter I and Mpimbaza, Arthur and Kiggundu, Moses and Sserwanga, Asadu and Kapisi, James and Arinaitwe, Emmanuel and Gonahasa, Samuel and Opigo, Jimmy and Ebong, Chris and Staedke, Sarah G and Shililu, Josephat and Okia, Michael and Rutazaana, Damian and Maiteki-Sebuguzi, Catherine and Belay, Kassahun and Kamya, Moses R and Dorsey, Grant and Rodriguez-Barraquer, Isabel},
  date = {2021-05},
