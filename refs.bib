@article{ChaccourCJ2018TargetingCattle,
  title = {Targeting Cattle for Malaria Elimination: Marked Reduction of {{Anopheles}} Arabiensis Survival for over Six Months Using a Slow-Release Ivermectin Implant Formulation},
  author = {Chaccour, Carlos J and Ngha'bi, Kija and Abizanda, Gloria and Irigoyen Barrio, Angel and Aldaz, Azucena and Okumu, Fredros and Slater, Hannah and Del Pozo, Jose Luis and Killeen, Gerry},
  date = {2018-05},
  journaltitle = {Parasit. Vectors},
