@article{OtamboWO2022InfluenceLandscape,
  title = {Influence of Landscape Heterogeneity on Entomological and Parasitological Indices of Malaria in {{Kisumu}}, {{Western Kenya}}},
  author = {Otambo, Wilfred Ouma and Onyango, Patrick O. and Wang, Chloe and Olumeh, Julius and Ondeto, Benyl M. and Lee, Ming-Chieh and Atieli, Harrysone and Githeko, Andrew K. and Kazura, James and Zhong, Daibin and Zhou, Guofa and Githure, John and Ouma, Collins and Yan, Guiyun},
  date = {2022-09-27},
  journaltitle = {Parasites \& Vectors},
