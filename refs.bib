@article{SmithDL2013StickySituation,
  title = {A Sticky Situation: The Unexpected Stability of Malaria Elimination},
  author = {Smith, David L and Cohen, Justin M and Chiyaka, Christinah and Johnston, Geoffrey and Gething, Peter W and Gosling, Roly and Buckee, Caroline O and Laxminarayan, Ramanan and Hay, Simon I and Tatem, Andrew J},
  date = {2013-08},
  journaltitle = {Philos. Trans. R. Soc. Lond. B Biol. Sci.},
  volume = {368},
  number = {1623},
