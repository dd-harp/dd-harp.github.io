}

@article{AsareEO2017AssessingClimate,
  title = {Assessing {{Climate Driven Malaria Variability}} in {{Ghana Using}} a {{Regional Scale Dynamical Model}}},
  author = {Asare, Ernest O. and Amekudzi, Leonard K.},
