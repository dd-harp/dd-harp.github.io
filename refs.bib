@article{SubramanianS2004DynamicsWuchereria,
  title = {The Dynamics of {{{\emph{Wuchereria}}}}{\emph{ Bancrofti}} Infection: A Model-Based Analysis of Longitudinal Data from {{Pondicherry}}, {{India}}},
  shorttitle = {The Dynamics of {{{\emph{Wuchereria}}}}{\emph{ Bancrofti}} Infection},
  author = {Subramanian, S. and Stolk, W. A. and Ramaiah, K. D. and Plaisier, A. P. and Krishnamoorthy, K. and Van Oortmarssen, G. J. and Amalraj, D. Dominic and Habbema, J. D. F. and Das, P. K.},
