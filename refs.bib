  title = {Biologically Meaningful Coverage Indicators for Eliminating Malaria Transmission},
  author = {Kiware, Samson S and Chitnis, Nakul and Devine, Gregor J and Moore, Sarah J and Majambere, Silas and Killeen, Gerry F},
