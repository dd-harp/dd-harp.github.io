@article{KleinEY2008ClinicallyImmune,
  title = {Clinically Immune Hosts as a Refuge for Drug-Sensitive Malaria Parasites},
  author = {Klein, Eili Y and Smith, David L and Boni, Maciej F and Laxminarayan, Ramanan},
  date = {2008-04},
  journaltitle = {Malar. J.},
  volume = {7},
