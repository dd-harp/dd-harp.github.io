@article{DennyM2019PerformanceVariable,
  title = {Performance in a Variable World: Using {{Jensen}}'s Inequality to Scale up from Individuals to Populations},
  author = {Denny, Mark},
  date = {2019-09},
  journaltitle = {Conserv Physiol},
  volume = {7},
  number = {1},
  eprint = {31528348},
