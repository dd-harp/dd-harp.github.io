@article{ElyazarIRF2013DistributionBionomics,
  title = {The Distribution and Bionomics of Anopheles Malaria Vector Mosquitoes in Indonesia},
  author = {Elyazar, Iqbal R F and Sinka, Marianne E and Gething, Peter W and Tarmidzi, Siti N and Surya, Asik and Kusriastuti, Rita and {Winarno} and Baird, J Kevin and Hay, Simon I and Bangs, Michael J},
