  keywords = {malaria infection model,MBPT dynamics and control,Plasmodium falciparum},
  file = {/Users/smitdave/Zotero/storage/L2JJZVX4/Peters et al. 2002 - High diversity and rapid changeover of expressed va ... e acute phase of Plasmodium falciparum infections in human volunteers.pdf;/Users/smitdave/Zotero/storage/QW2CX89H/Peters et al. 2002 - High diversity and rapid changeover of expressed va ... e acute phase of Plasmodium falciparum infections in human volunteers.pdf}
