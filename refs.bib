@article{DuffyCF2001EffectsDietary,
  title = {Effects of Dietary Supplementation with {{Yucca}} Schidigera {{Roezl}} Ex {{Ortgies}} and Its Saponin and Non-Saponin Fractions on Rat Metabolism.},
