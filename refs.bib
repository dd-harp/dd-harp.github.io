@article{TatemAJ2006GlobalTransport,
  title = {Global Transport Networks and Infectious Disease Spread},
  author = {Tatem, Andrew J and Rogers, D J and Hay, Simon I},
  date = {2006},
  journaltitle = {Adv. Parasitol.},
  volume = {62},
  pages = {293--343},
