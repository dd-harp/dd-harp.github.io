@article{MoonenB2010FrameworkAssessing,
  title = {A Framework for Assessing the Feasibility of Malaria Elimination},
  author = {Moonen, Bruno and Cohen, Justin M and Tatem, Andy J and Cohen, Jessica and Hay, Simon I and Sabot, Oliver and Smith, David L},
  date = {2010-11},
  journaltitle = {Malar. J.},
