  file = {/Users/smitdave/Zotero/storage/2GGZZAYC/Howes et al. 2011 - 54D0C0CD-88F5-4D7B-B78B-29A42200ADE9.pdf;/Users/smitdave/Zotero/storage/RRYAYKSB/Howes et al. 2011 - The global distribution of the Duffy blood group.pdf}
