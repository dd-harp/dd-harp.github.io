@article{BejonP2010StableUnstable,
  title = {Stable and Unstable Malaria Hotspots in Longitudinal Cohort Studies in {{Kenya}}},
  author = {Bejon, Philip and Williams, Thomas Neil and Liljander, Anne and Noor, Abdisalan Mohamed and Wambua, Juliana and Ogada, Edna and Olotu, Ally and Osier, Faith H A and Hay, Simon I and Färnert, Anna and Marsh, Kevin},
  date = {2010-07},
