@article{MartensP2003FutureHealth,
  title = {A Future without Health? {{Health}} Dimension in Global Scenario Studies},
  author = {Martens, Pim and Huynen, Maud},
  date = {2003},
  journaltitle = {Bull. World Health Organ.},
  volume = {81},
