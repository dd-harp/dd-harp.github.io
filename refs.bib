@article{PortugalS2011HostmediatedRegulation,
  title = {Host-Mediated Regulation of Superinfection in Malaria},
  author = {Portugal, Silvia and Carret, Céline and Recker, Mario and Armitage, Andrew E and Gonçalves, Lígia A and Epiphanio, Sabrina and Sullivan, David and Roy, Cindy and Newbold, Chris I and Drakesmith, Hal and Mota, Maria M},
  date = {2011-06},
  journaltitle = {Nat. Med.},
  volume = {17},
  number = {6},
  eprint = {21572427},
  eprinttype = {pmid},
  pages = {732--737},
  publisher = {{Nature Publishing Group}},
  issn = {1078-8956, 1546-170X},
  doi = {10.1038/nm.2368},
  url = {http://dx.doi.org/10.1038/nm.2368},
  abstract = {In regions of high rates of malaria transmission, mosquitoes repeatedly transmit liver-tropic Plasmodium sporozoites to individuals who already have blood-stage parasitemia. This manifests itself in semi-immune children (who have been exposed since birth to Plasmodium infection and as such show low levels of peripheral parasitemia but can still be infected) older than 5 years of age by concurrent carriage of different parasite genotypes at low asymptomatic parasitemias. Superinfection presents an increased risk of hyperparasitemia and death in less immune individuals but counterintuitively is not frequently observed in the young. Here we show in a mouse model that ongoing blood-stage infections, above a minimum threshold, impair the growth of subsequently inoculated sporozoites such that they become growth arrested in liver hepatocytes and fail to develop into blood-stage parasites. Inhibition of the liver-stage infection is mediated by the host iron regulatory hormone hepcidin, whose synthesis we found to be stimulated by blood-stage parasites in a density-dependent manner. We mathematically modeled this phenomenon and show how density-dependent protection against liver-stage malaria can shape the epidemiological patterns of age-related risk and the complexity of malaria infections seen in young children. The interaction between these two Plasmodium stages and host iron metabolism has relevance for the global efforts to reduce malaria transmission and for evaluation of iron supplementation programs in malaria-endemic regions.},
  langid = {english},
  keywords = {bib\_from\_DG.ris,inefficient transmission,Iron,iron games,malaria,malaria infection model,Parasite host response,Parasitic infection,Pathogens,PfEIR vs. PfPR,power law,transmission efficiency},
  file = {/Users/smitdave/Zotero/storage/8GNEMRS6/Portugal et al. (2011) Host-mediated regulation of superinfection - Supplement.pdf;/Users/smitdave/Zotero/storage/I4AK99M7/Portugal et al. (2011) Host-mediated regulation of superinfection in mala.pdf}
}

@report{PothinE2017MethodologyMalaria,
