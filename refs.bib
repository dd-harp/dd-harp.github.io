@article{SmithDL2010ProgressModelling,
  title = {Progress in Modelling Malaria Transmission},
  author = {Smith, David L and Ruktanonchai, Nick},
  date = {2010},
  journaltitle = {Advances in experimental medicine and biology},
  shortjournal = {Advances in experimental medicine and biology},
  volume = {673},
  eprint = {20632526},
  eprinttype = {pubmed},
  pages = {1--12},
  url = {http://www.ncbi.nlm.nih.gov/pubmed/20632526},
  abstract = {Transmission of human malaria is a complicated dynamic process that involves populations of humans, parasites, and vectors. The first mathematical models of malaria are now more than a century old, and they are still a useful conceptual synthetic description of transmission, but they fail in some important ways. To address some of those failures, malaria transmission models have now been extended to consider malaria immunity, superinfection, and heterogeneous biting, among other factors. These extensions of the basic theory often arise from field studies in a single place, but tests of the theory come comparing standard measures of malaria taken from many places across the transmission spectrum. Several good models now exist that describe these basic patterns across the spectrum from low to high endemicity. The future of malaria modeling will involve applying these models to make decisions about real systems and finding new ways to test the underlying causes of the patterns.},
  keywords = {article,Bibliographies/Books,commentary,DLS,malaria,MBPT dynamics and control,mine,Systematic Review (1970-2010)},
  file = {/Users/smitdave/Zotero/storage/3HWPXTA7/Smith and Ruktanonchai - 2010 - Progress in modelling malaria transmission.pdf;/Users/smitdave/Zotero/storage/EUQS84G9/Smith and Ruktanonchai - 2010 - Progress in modelling malaria transmission.pdf;/Users/smitdave/Zotero/storage/FPLIB789/Smith and Ruktanonchai 2010 - Progress in modelling malaria transmission.pdf}
}

@article{SmithDL2010ProspectiveStrategies,
  title = {Prospective Strategies to Delay the Evolution of Anti-Malarial Drug Resistance: Weighing the Uncertainty},
  author = {Smith, David L and Klein, Eili Y and McKenzie, F Ellis and Laxminarayan, Ramanan},
  date = {2010},
  journaltitle = {Malar J},
  shortjournal = {Malaria J},
  volume = {9},
  number = {1},
  pages = {217},
  doi = {10.1186/1475-2875-9-217},
  url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=20653960},
  abstract = {ABSTRACT: BACKGROUND: The evolution of drug resistance in malaria parasites highlights a need to identify and evaluate strategies that could extend the useful therapeutic life of anti-malarial drugs. Such strategies are deployed to best effect before resistance has emerged, under conditions of great uncertainty. METHODS: Here, the emergence and spread of resistance was modelled using a hybrid framework to evaluate prospective strategies, estimate the time to drug failure, and weigh uncertainty. The waiting time to appearance was estimated as the product of low mutation rates, drug pressure, and parasite population sizes during treatment. Stochastic persistence and the waiting time to establishment were simulated as an evolving branching process. The subsequent spread of resistance was simulated in simple epidemiological models. RESULTS: Using this framework, the waiting time to the failure of artemisinin combination therapy (ACT) for malaria was estimated, and a policy of multiple first-line therapies (MFTs) was evaluated. The models quantify the effects of reducing drug pressure in delaying appearance, reducing the chances of establishment, and slowing spread. By using two first-line therapies in a population, it is possible to reduce drug pressure while still treating the full complement of cases. CONCLUSIONS: At a global scale, because of uncertainty about the time to the emergence of ACT resistance, there was a strong case for MFTs to guard against early failure. Our study recommends developing operationally feasible strategies for implementing MFTs, such as distributing different ACTs at the clinic and for home-based care, or formulating different ACTs for children and adults.},
  keywords = {antimalarial drug resistance,article,Bibliographies/Books,bioeconomics,DLS,evolution,Evolution,malaria,MBPT dynamics and control,mine,Systematic Review (1970-2010)},
  file = {/Users/smitdave/Zotero/storage/54PUU29R/Smith et al. 2010 - Prospective strategies to delay the evolution of anti-malarial drug resistance - weighing the uncertainty.pdf;/Users/smitdave/Zotero/storage/GNP4JDNV/Smith et al. - 2010 - Prospective strategies to delay the evolution of a.pdf;/Users/smitdave/Zotero/storage/T4F3H3K6/Smith et al. - 2010 - Prospective strategies to delay the evolution of a.pdf;/Users/smitdave/Zotero/storage/TBXEJCRD/Smith et al. 2010 - 544D0D10-A1BC-4E0F-966C-4E0FC543805C.pdf}
}

@article{SmithDL2011_Sisyphus,
  title = {Infectious Disease. {{Solving}} the {{Sisyphean}} Problem of Malaria in {{Zanzibar}}},
  author = {Smith, David L and Cohen, Justin M and Moonen, Bruno and Tatem, Andrew J and Sabot, Oliver J and Ali, Abdullah and Mugheiry, Sultan M},
  date = {2011-06},
  journaltitle = {Science},
  volume = {332},
  number = {6036},
  eprint = {21680828},
  eprinttype = {pmid},
  pages = {1384--1385},
  publisher = {{American Association for the Advancement of Science}},
  issn = {0036-8075, 1095-9203},
  doi = {10.1126/science.1201398},
  url = {http://dx.doi.org/10.1126/science.1201398},
  abstract = {The Global Malaria Action Plan (GMAP), a consensus framework for coordinated action, aims to end malaria deaths by 2015 and eventually to eradicate malaria ([ 1 ][1]). The plan calls for universal access to effective antimalarial drugs and universal coverage with appropriate vector interventions. Strategic planning for how best to reach these goals has been left to individual countries, some of which have already made plans to eliminate malaria, i.e., to rid their countries of malaria parasites and to suppress transmission from imported malaria (travelers carrying malaria infections from one region into another) so that locally acquired cases are rare ([ 2 ][2]). Critics have argued that plans for national elimination distract attention and resources from the priority of reducing malaria's heavy burden in sub-Saharan Africa ([ 3 ][3]) and that a better strategy would be “control,” i.e., reducing malaria to a minor public health problem. These sides reflect the bipolar history of antimalaria efforts. When funding collapsed for a previous attempt to eradicate malaria, control defined the malaria agenda through decades of neglect. Control and elimination are often presented as opposite sides of a debate over how to allocate billions of dollars allocated globally for malaria aid. But a recent study in Zanzibar ([ 4 ][4]) concluded the dichotomy was false. A more urgent problem is continuity. How can enthusiasm for funding malaria be sustained? Since 2002, Zanzibar has scaled up intervention coverage and sharply reduced malaria ([ 5 ][5], [ 6 ][6]). Zanzibar must now decide how to consolidate success, as other countries that succeed in reducing malaria must also do. To help set an appropriate strategy, Zanzibar commissioned a report to assess the feasibility of malaria elimination ([ 4 ][4]), with the goal of comparing requirements for elimination and control (i.e., sustaining current interventions). Elimination would allow Zanzibar to scale back some interventions and, once malaria was sufficiently rare, to transition to a system of active case detection and outbreak response ([ 4 ][4]). This would require substantial intensification of surveillance and building capacity but increase costs from the current control program by less than \$1 per capita ([ 7 ][7]). If imported malaria cases dropped, costs would also decline, and elimination would then cost less than control. Despite operational differences, long-term implications of both strategies were found to be similar. In both cases, interventions had to be maintained to prevent imported malaria from leading to a resumption of local transmission. In other words, the conclusion was not that control or elimination would be a better strategy; it was that regardless of what Zanzibar chooses to do, malaria will remain a threat for many years. The problem remains: how to continue funding a program to manage the risk of resurgence that will persist until the threat of malaria is gone. Zanzibar has been at these crossroads before. The capital, Stone Town, was called a “hot bed of malaria” ([ 8 ][8]). Zanzibar had reduced malaria to very low levels by 1969 ([ 9 ][9]), but with malaria no longer a visible threat to health, interventions were abandoned in 1970. Maintaining a minimal malaria burden despite the continued presence of mosquitoes and other conditions that make an area receptive to malaria requires active suppression of transmission, so malaria returned to levels seen before control (see the graph, opposite page). In 2002, before scale-up of interventions, malaria transmission in Zanzibar was as intense as anywhere in Africa. Without maintaining programs that minimize the burden today, malaria would return again to Zanzibar. This history recalls the myth of Sisyphus, who was forced by Zeus to push a boulder up a hill only to have it roll back down and therefore suffer an eternity of frustration. Malaria's curse is caused by a common, but pernicious, public health conundrum: the greater the success in suppressing an infectious disease, the lower the perceived threat, and the more challenging it becomes to maintain funding and to sustain control. Zanzibar's success shows what is possible and can help build enthusiasm and support for reducing malaria elsewhere. However, the challenge of sustaining success against an imminent but invisible threat must be solved. Failure in Zanzibar would raise questions about the long-term prospects elsewhere in Africa. Zanzibar's dilemma will soon arise in each region that is newly successful in reducing the burden of malaria with the help of donor funding; the uncertainties in Zanzibar are the uncertainties of our global malaria strategy. Zanzibar's malaria funding is once again threatened. International donors are considering the allocation of limited budgets against the few thousand malaria cases that still occur there. The islands are home to only 1.2 million people out of 678 million Africans at risk of malaria ([ 10 ][10]), most of whom also require malaria interventions under the GMAP. Yet the belief that it is possible to shift malaria funds away from Zanzibar arises from basic misunderstandings. First, intensive interventions must remain in place to suppress transmission, or malaria will rapidly return to its highly endemic and high burden baseline, just as it has in the past (see the graph). By analogy, measles epidemics are inevitable if vaccination coverage declines. Malaria interventions must become standard public health interventions, like vaccines. Second, it makes no sense to stop protecting 1.2 million Zanzibaris now that the program has achieved success. Because the costs of reducing malaria greatly depend on drugs, nets, insecticides, and other fixed-price commodities, budgets are roughly proportional to population size, and moving money from Zanzibar would simply protect an equivalent number of citizens in a more populous country. Reshuffling resources among countries, repeatedly sacrificing hard-earned gains to chase new ones, would indeed curse all of Africa with the Sisyphean malaria cycles seen in Zanzibar. ![Figure][11] A plot of malaria parasite surveys in Zanzibar since 1957. Large regional or national surveys (large symbols) and smaller localized surveys (small symbols) of Unguja (purple), Pemba (green), or the entire archipelago (black). Prevalence estimates reported from various age ranges have been standardized to children aged 2 to 10 years ([ 15 ][12]). The first intensive program was part of the Global Malaria Eradication Program (GMEP). The second intensive program, conducted during the 1990s by the Zanzibar Malaria Control Programme (ZMCP) and the U.S. Agency for International Development (USAID), halved prevalence but did not come close to elimination. No extensive surveys were conducted on Unguja until after the current intensive program began. The time span of the three control programs is less shaded. The current intensive program is sponsored by the ZMCP, the U.S. President's Malaria Initiative (PMI); and The Global Fund to Fight AIDS, Tuberculosis, and Malaria (GFATM). (See SOM for details such as error bars.) Finally, it is a mistake to judge the return on malaria investments in relation to the current burden of malaria. A facile weighting of the millions invested in malaria interventions against a few remaining cases produces the erroneous conclusion that those funds are achieving little. The true value of these investments is the number of deaths and cases that are prevented, not that remain. In Zanzibar each year, benefits include prevention of ∼660,000 malaria cases and 3300 malaria-attributable deaths that occurred before control; at the current average control cost of \$3.9 million per year ([ 11 ][13]), this translates into \$1183 per death averted and \$34.5 per disability-adjusted life year averted, in line with the world's most cost-effective public health interventions \textbackslash textbackslash[supporting online material (SOM)\textbackslash textbackslash] ([ 12 ][14]). In Zanzibar, sustained implementation of malaria interventions that make this possible continues to save lives, and it is as cost-effective to continue programs there as it would be to implement a new program at a cost of \$3.9 million per year to prevent nearly 660,000 cases each year somewhere else. For Africa to replicate the success seen in Zanzibar and to achieve the goals of the GMAP, funding levels must match or exceed the current per-capita spending levels in Zanzibar; this investment amounts to at least \$2.2 billion per year to cover 678 million Africans at risk ([ 10 ][10]). The GMAP estimates that almost \$6 billion per year are required globally, but total commitments currently amount to less than \$2 billion per year ([ 13 ][15]). The crux of the problem is that current funding levels are too low to fulfill the GMAP goal of scaling up coverage levels everywhere it is needed, but malaria interventions cannot be scaled back in places like Zanzibar while the threat of malaria remains. The obvious answer is that more money is needed over the next funding cycles to secure the gains that have been made while extending malaria control to new regions. With the severe resource constraints that currently exist, however, the global malaria community needs to define a coherent and efficient long-term strategy. Zanzibar's elimination feasibility report ([ 4 ][4]) points to one viable path, only if the threat of malaria resurgence diminishes. Without renewed transmission from endemic pockets of malaria within the borders or large numbers of imported infections from abroad, countries could scale back current interventions and adopt a system of surveillance and outbreak response. Zanzibar's costs would also be reduced by suppressing malaria transmission in nearby Kenya and Tanzania to reduce imported malaria ([ 4 ][4]). Reducing malaria creates a benefit for neighbors that did not pay for it. Scaling up interventions in regionally coordinated programs, such as those implemented in Southern Africa and Southeast Asia, could increase malaria security for every country in the region, reinforce national success, reduce overall costs, and minimize risks. These regional control zones could be expanded, with additional funds leapfrogged to new countries to advance the front. Through such a spatially progressive approach ([ 14 ][16]), funds could be shifted from successful countries in a strategic, logical way without incurring enormous risks. Zanzibar's achievement must be sustained and integrated into a coherent long-term continental strategy that shrinks Africa's malaria map ([ 14 ][16]). Implementing these programs will require stable long-term funding commitments from donors to sustain the effort over several decades. Countries also must do their part and eventually reduce their reliance on external funding for their malaria programs through predictable domestic funding mechanisms such as an earmarked tax or trust funds ([ 4 ][4]). Malaria endemic countries are not fated to watch their triumph collapse back into failure. Zanzibar's early success shows what can be achieved in Africa. It has proven it can efficiently utilize international resources to reduce malaria dramatically, but funding must be sustained for the next decade or more until a permanent solution to the malaria problem is found. Funders need only to realize that success is possible and that building on the gains being made today is one of the best deals in global health. [www.sciencemag.org/cgi/content/full/332/6036/1384/DC1][17] 1. [↵][18] Roll Back Malaria Partnership (RBMP), The Global Malaria Action Plan for a Malaria-Free World (RBMP, World Health Organization, Geneva, 2008). 2. [↵][19] 1. J. M. Cohen 2. et al ., Malar. J. 9, 213 (2010). [OpenUrl][20][CrossRef][21][PubMed][22] 3. [↵][23] 1. R. W. Snow, 2. K. Marsh , Lancet 376, 137 (2010). [OpenUrl][24][CrossRef][25][PubMed][26][Web of Science][27] 4. [↵][28] Zanzibar Malaria Control Programme (ZMCP), Malaria Elimination in Zanzibar: A Feasibility Assessment (ZMCP, Zanzibar, 2009). 5. [↵][29] ZMCP, “Roll Back Malaria Indicator Survey 2007 main report” (ZMCP, Zanzibar, 2008). 6. [↵][30] ZMCP, “Roll Back Malaria Indicator Survey 2010 main report” (ZMCP, Zanzibar, 2011). 7. [↵][31] 1. B. Moonen 2. et al ., Lancet 376, 1592 (2010). [OpenUrl][32][CrossRef][33][PubMed][34][Web of Science][35] 8. [↵][36] 1. A. Oakes, 2. W. Maycock , British and Foreign State Papers, 1895–1896 (Harrison and Sons, London, 1900). 9. [↵][37] 1. Y. G. Matola 2. et al ., Cent. Afr. J. Med. 30, 95 (1984). [PubMed][38] [OpenUrl][39] 10. [↵][40] 1. S. I. Hay 2. et al ., PLoS Med. 6, e1000048 (2009). [OpenUrl][41][PubMed][42] 11. [↵][43] 1. O. Sabot 2. et al ., Lancet 376, 1604 (2010). [OpenUrl][44][CrossRef][45][PubMed][46][Web of Science][47] 12. [↵][48] 1. R. Laxminarayan 2. et al ., Lancet 367, 1193 (2006). [OpenUrl][49][CrossRef][50][PubMed][51][Web of Science][52] 13. [↵][53] World Health Organization, World Malaria Report 2010 (WHO, Geneva, 2010). 14. [↵][54] 1. R. G. A. Feachem, 2. A. A. Phillips, 3. G. A. Targett , Eds., Shrinking the Malaria Map: A Prospectus on Malaria Elimination (The Global Health Group, Univ. of California San Francisco, San Francisco, 2009). 15. [↵][55] 1. D. L. Smith, 2. C. A. Guerra, 3. R. W. Snow, 4. S. I. Hay , Malar. J. 6, 131 (2007). [OpenUrl][56][CrossRef][57][PubMed][58] 16. The authors thank the Bill \& Melinda Gates Foundation for financial support, and H. Gelband and S. Hay for feeback. [1]: \#ref-1 [2]: \#ref-2 [3]: \#ref-3 [4]: \#ref-4 [5]: \#ref-5 [6]: \#ref-6 [7]: \#ref-7 [8]: \#ref-8 [9]: \#ref-9 [10]: \#ref-10 [11]: pending:yes [12]: \#ref-15 [13]: \#ref-11 [14]: \#ref-12 [15]: \#ref-13 [16]: \#ref-14 [17]: http://www.sciencemag.org/cgi/content/full/332/6036/1384/DC1 [18]: \#xref-ref-1-1 “View reference 1 in text” [19]: \#xref-ref-2-1 “View reference 2 in text” [20]: \{openurl\}?query=rft.