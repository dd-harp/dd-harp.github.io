@article{YeY2009LocalScale,
  title = {Local Scale Prediction of {{Plasmodium}} Falciparum Malaria Transmission in an Endemic Region Using Temperature and Rainfall},
  author = {Yé, Yazoumé and Hoshen, Moshe and Kyobutungi, Catherine and Louis, Valérie R. and Sauerborn, Rainer},
  date = {2009-11-01},
  journaltitle = {Global Health Action},
  volume = {2},
