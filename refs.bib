@article{SharmaSK2004EpidemiologyMalaria,
  title = {Epidemiology of Malaria Transmission and Development of Natural Immunity in a Malaria-Endemic Village, {{San Dulakudar}}, in {{Orissa}} State, {{India}}},
  author = {Sharma, Surya K and Chattopadhyay, Rana and Chakrabarti, Kausik and Pati, Sudhanshu S and Srivastava, Vinod K and Tyagi, Prajesh K and Mahanty, Sanjib and Misra, Saroj K and Adak, Tridibes and Das, Bhabani S and Chitnis, Chetan E},
  date = {2004-10},
  journaltitle = {Am. J. Trop. Med. Hyg.},
