@article{MoyesCL2013ProvidingOpen,
  title = {Providing Open Access Data Online to Advance Malaria Research and Control},
  author = {Moyes, Catherine L and Temperley, William H and Henry, Andrew J and Burgert, Clara R and Hay, Simon I},
