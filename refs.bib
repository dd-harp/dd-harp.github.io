@article{HaqueU2013RiskFactors,
  title = {Risk Factors Associated with Clinical Malaria Episodes in {{Bangladesh}}: A Longitudinal Study},
  author = {Haque, Ubydul and Glass, Gregory E and Bomblies, Arne and Hashizume, Masahiro and Mitra, Dipak and Noman, Nawajish and Haque, Waziul and Kabir, M Moktadir and Yamamoto, Taro and Overgaard, Hans J},
