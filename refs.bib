@article{ServiceMW1997_MosquitoDispersal,
  title = {Mosquito ({{Diptera}}: {{Culicidae}}) Dispersal–the Long and Short of It},
  author = {{Service, M W}},
  date = {1997-11},
  journaltitle = {J. Med. Entomol.},
