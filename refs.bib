@article{WoottonJC2002GeneticDiversity,
  title = {Genetic Diversity and Chloroquine Selective Sweeps in {{Plasmodium}} Falciparum},
  author = {Wootton, John C and Feng, Xiaorong and Ferdig, Michael T and Cooper, Roland A and Mu, Jianbing and Baruch, Dror I and Magill, Alan J and Su, Xin-Zhuan},
  date = {2002-07},
  journaltitle = {Nature},
  volume = {418},
  number = {6895},
