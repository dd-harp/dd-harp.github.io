  keywords = {Anemia,Animals,{Antigens, Protozoan},Asembo Bay,Cohort Studies,Cross-Sectional Studies,Hemoglobins,Humans,Infant,Interferon-gamma,Lymphocyte Activation,{Malaria, Falciparum},Plasmodium falciparum,Protozoan Vaccines},
