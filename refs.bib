  author = {Mlacha, Yeromin P. and Chaki, Prosper P. and Malishee, Alpha D. and Mwakalinga, Victoria M. and Govella, Nicodem J. and Limwagu, Alex J. and Paliga, John M. and Msellemu, Daniel F. and Mageni, Zawadi D. and Terlouw, Dianne J. and Killeen, Gerry F. and Dongus, Stefan},
  date = {2017-05-11},
  journaltitle = {Geospatial health},
