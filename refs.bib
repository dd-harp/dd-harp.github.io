@article{LloydAL2007StochasticityHeterogeneity,
  title = {Stochasticity and Heterogeneity in Host–Vector Models},
  author = {Lloyd, Alun L and Zhang, Ji and Root, A Morgan},
  date = {2007},
  journaltitle = {Journal of the Royal Society Interface},
  shortjournal = {Journal of the Royal Society, Interface / the Royal Society},
  volume = {4},
