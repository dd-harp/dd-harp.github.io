@article{SmithNR2018AgentbasedModels,
  title = {Agent-Based Models of Malaria Transmission: {{A}} Systematic Review},
  author = {Smith, Neal R and Trauer, James M and Gambhir, Manoj and Richards, Jack S and Maude, Richard J and Keith, Jonathan M and Flegg, Jennifer A},
  date = {2018-08},
  journaltitle = {Malar. J.},
  volume = {17},
